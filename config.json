{
  "locals": {
    "url": "http://localhost:8080",
    "name": "CivicSpaces",
    "owner": "CivicSpaces",
    "description": "CivicSpaces is a way to connect active citizens to unused spaces in their city."
  },
  "plugins": [
    "wintersmith-sass",
    "wintersmith-livereload"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  }
}
